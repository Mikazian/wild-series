<!DOCTYPE html>
<html data-bs-theme="light">
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Wild Series@
			{% endblock %}
		</title>
		<link
		rel="icon" href="build/images/icone.ico"/>
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		{% block nav %}
			<header>
				{% include './componant/_navbar.html.twig'%}
			</header>
		{% endblock %}
		{% block body %}
			{# {{ app.session.get('total') }} #}
			{% for category, messages in app.flashes %}
				{% for message in messages %}
					<div class="alert alert-{{ category }}">
						{{ message }}
					</div>
				{% endfor %}
			{% endfor %}
		{% endblock %}
		{% block footer %}
			{% include './componant/_footer.html.twig'%}
		{% endblock %}
	</body>
</html>
