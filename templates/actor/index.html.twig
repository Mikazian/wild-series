{% extends 'base.html.twig' %}

{% block title %}Wild Series | Index
{% endblock %}

{% block body %}
	<h1 class="text-primary">Tous les acteurs :</h1>
	<ul>
		{% for actor in actors %}
			<li class="text-light">
				<a href="{{ path('actor_show', {'id' : actor.id}) }}">{{ actor.name }}</a>
			</li>
		{% endfor %}
	</ul>
{% endblock %}
